<% include ./partials/header.ejs %>
<div class="container">
<div class="form-group" style="width:30%;margin:30px auto">
<form action="/register" method="POST">
    <div class="form-group">
    <label>Username</label>
    <input type="text" name ="username" class="form-control" required placeholder ="enter the username">
    </div>
    <div class="form-group">
    <label>Password</label>
    <input type="password" name ="password" class="form-control" minlength="5" maxlength="10" required placeholder ="enter the password">
    </div>
    <button class="btn btn-primary">Submit</button>
</form>
</div>
</div>
<% include ./partials/footer.ejs %>